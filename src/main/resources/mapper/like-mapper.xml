<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blog.dao.LikeMapper">

    <select id="getUserLikeVo" resultType="com.blog.entity.Vo.LikeVo">
        select accountId,username,header_url from user where accountId = #{accountId}
    </select>
</mapper>